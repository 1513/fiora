    "buildPc": "export NODE_ENV=production && ./node_modules/.bin/webpack -p --config webpack/webpack.prod.js --entry ./src/client/webPc/index.js --output-filename app.js",
    "buildMobile": "export NODE_ENV=production && ./node_modules/.bin/webpack -p --config webpack/webpack.prod.js --entry ./src/client/webMobile/index.js --output-filename app.mobile.js",
    "rnServer": "node node_modules/react-native/local-cli/cli.js start --root src/client/rnMobile --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "./node_modules/.bin/eslint ./ --cache --ignore-pattern .eslintignore --fix"
