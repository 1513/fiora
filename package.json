    "start": "(npm run devPc &) && (npm run devMobile &) && (node bin/reduxDevServer.js &) && node bin/develop.js",
    "devPc": "export NODE_ENV=development && ./node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.js --port 8080 --entry ./src/client/webPc/index.js --output-filename ./app.js",
